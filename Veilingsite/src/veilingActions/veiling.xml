<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="visitor" extends="struts-default">
		<action name="UserList" class="veilingActions.visitor.UserList">
			<result name="success">/jsp/users.jsp</result>
		</action>
		<action name="Register" class="veilingActions.visitor.Register">
			<result name="input">/jsp/register.jsp</result>
			<result name="success">/jsp/index.jsp</result>
		</action>
		
	</package>
</struts>